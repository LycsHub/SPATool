                              <Label layoutX="351.0" layoutY="339.0" prefHeight="15.0" prefWidth="81.0" text="Save File Type">
                                 <font>
                                    <Font size="11.0" />
                                 </font></Label>
                              <Button onAction="#ASTFile" id="ASTFile" layoutX="342.0" layoutY="128.0" mnemonicParsing="false" prefHeight="23.0" prefWidth="100.0" text="Select File" />
