                              <ImageView id="ASTImage" fitHeight="760.0" fitWidth="542.0" layoutX="449.0" layoutY="8.0" pickOnBounds="true" preserveRatio="true" />
                              <Button id="ASTSave" layoutX="342.0" layoutY="209.0" mnemonicParsing="false" onAction="#ASTSave" prefHeight="23.0" prefWidth="100.0" text="Save AST File" textAlignment="CENTER">
                                 <font>
                                    <Font size="11.0" />
                                 </font></Button>
                              <Button id="ASTExport" layoutX="342.0" layoutY="253.0" mnemonicParsing="false" onAction="#ASTExport" prefHeight="23.0" prefWidth="100.0" text="Export Image">
                                 <font>
                                    <Font size="11.0" />
                                 </font></Button>
                              <Button id="ASTReset" layoutX="342.0" layoutY="298.0" mnemonicParsing="false" onAction="#ASTReset" prefHeight="23.0" prefWidth="98.0" text="Reset" textAlignment="CENTER" />
