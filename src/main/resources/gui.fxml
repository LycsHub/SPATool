                <Tab text="Controller Flow Graph">
<!--                <Tab text="CFG">-->
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="894.0" prefWidth="1000.0">
                           <children>
                              <ToolBar prefHeight="44.0" prefWidth="1000.0">
                                <items>
                                  <Button id="CFGFile" mnemonicParsing="false" onAction="#CFGFile" text="Select Class File" textAlignment="CENTER" textFill="#050805" />
